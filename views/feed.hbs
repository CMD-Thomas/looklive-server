    {{#each items}}
        <div class="feed-item" data-uuid="{{id}}">
            <img class="feed__img" src="{{ image.url }}">
            <h2 class="feed__subtitle"><img class="avatar" src="{{ subject.avatar.url }}"> {{ subject.name }}</h2>
            <h1 class="feed__title"><a class="feed__link" href="appearance/{{id}}">{{title}}</a></h1>
        </div>
    {{/each}}